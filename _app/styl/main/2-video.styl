#video
	padding 90px 0 140px
	position relative
	background-color: #FAE34F
	background-image: url(../img/backs/video.png)
	background-repeat: no-repeat
	background-position: center
	background-size: cover
	overflow hidden
	@media screen and (max-width 991px)
		padding 60px 0 90px
	@media screen and (max-width 560px)
		padding 40px 0 90px
.video-container
	position relative
	z-index 30
	margin-top: 10px
	@media screen and (min-width 2295px)
		margin-top: 20px
	@media screen and (max-width 991px)
		text-align: center
		margin 10px 0 130px
	&-text
		display inline-block
		position absolute
		top 40px
		left -100px
		color #fff
		text-transform uppercase
		font-weight: bold
		background-color: #298d60
		padding 20px 30px
		border-radius: 8px
		letter-spacing: 1px
		z-index 30
		@media screen and (max-width 1199px)
			top -40px
		@media screen and (max-width 991px)
			position: relative
			top 0
			left 0
			margin-bottom 30px
	&-img
		position absolute
		bottom: 0
		right -60px
.video-wrapper
	margin 0 auto
	position relative
	width 100%
	top 0
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	&:after
		content: ''
		display block
		position: absolute
		top: 0
		left 0
		right 0
		width: 100%
		height 100px
	iframe
		position absolute
		width: 71%;
		height: 78%;
		display block
		background-color #000
		border-radius 0
		border: 0
		top 30px
		left 2px
		right 0
		bottom 0
		cursor pointer
		margin 0 auto
		@media screen and (max-width 385px)
			top 15px
	&-laptop
		position absolute
		top 0
		left 0
		right 0
		width 100%
.form-container
	position relative
	z-index 20
	&-img
		position absolute
		&-1
			top -50px
			left -35px
			@media screen and (max-width 560px)
				top -100px
		&-2
			bottom: -110px
			left -150px
			@media screen and (max-width 991px)
				bottom -130px
		&-3
			bottom: -70px
			right -230px
			@media screen and (max-width 991px)
				bottom -100px
.form
	padding 50px 65px
	background-color #fff
	border-radius: 7px 35px
	border 2px solid #9E9E9E
	box-shadow: 0 0 0 8px #fff, 0px 1px 24.3px 2.7px rgba(0, 0, 0, 0.17);
	width 100%
	max-width 570px
	margin 0 auto
	&-title
		font-size: 25px
		font-weight: bold
		text-transform uppercase
		text-align center
	@media screen and (max-width 560px)
		padding 30px 20px
		&-title
			font-size: 22px
.input
	background-color: transparent
	display block
	border: 1px solid #d2c2a0
	border-radius: 8px
	color: #b1acac;
	font-size: 16px;
	font-weight: normal;
	text-align left
	padding 25px 30px
	transition: all .3s ease
	width 100%
	max-width 320px
	margin 20px auto 10px
.button-form
	float: none
	display block
	margin 0 auto
	width 100%
	max-width 320px
@-webkit-keyframes autofill
	to
		font-size: 16px
		color: #b1acac;
		background-color: transparent
input:-webkit-autofill
	-webkit-animation-name: autofill;
	-webkit-animation-fill-mode: both;
.input:active
input:-internal-autofill-selected
	background-color: transparent !important
.alert-message
	clear both
	display: none;
	font-size: 16px;
	padding-top: 10px;
	color: #f00
	text-align: center
	font-weight: bold
	line-height: 1
.checking
	display: table;
	width 100%
	max-width 330px
	margin: 10px auto 0
	text-align: left
	@media screen and (max-width 1199px)
		margin 5px auto 0
	@media screen and (max-width 991px)
		max-width 380px
@keyframes pulse
	0%
		box-shadow: 0 0 0 0 rgba(red, .22)
	100%
		box-shadow: 0 0 15px 10px rgba(red, .22)
.checking .check-label
	line-height: 18px;
	cursor: pointer;
	position: relative;
	display: table-cell;
	vertical-align: middle;
	padding: 0 0 0 30px;
	text-align: left;
	font-size: 12px
	font-weight: 100
	color #8c8282
	@media screen and (max-width 991px)
		line-height: 20px
.checking .check-label a
	text-decoration: none
	border-bottom: 1px dashed
	display: inline
	color #8c8282
	transition: all .3s ease
	&:focus
		border-bottom: 1px dashed
		color #8c8282
	&:hover
		border-bottom: none
		text-decoration: none
		color #8c8282
.checking .check-label:before
	content: '';
	background-color: red;
	position: absolute;
	left: 0;
	top: 3px;
	width: 20px;
	height: 20px;
	border none
	border-radius: 0
	animation: pulse
	animation-duration: .9s
	animation-iteration-count: infinite
	animation-timing-function: ease-out;
.checking .check-label:after
	content: '';
	position: absolute;
	left: 7px;
	top: -10px;
	width: 8px;
	height: 14px;
	background-color: transparent;
	opacity: 0;
	border-bottom: 2px solid #fff;
	border-right: 2px solid #fff;
	transform: rotate(41deg);
	transition: all 0.5s ease;
.check
	position: absolute;
	z-index: -1;
	opacity: 0;
	width: 0;
	height 0;
	border: 0;
	left -99999px;
.check:checked + .check-label:before
	background-color: #FFBA00
	animation: none
.check:checked + .check-label:after
	opacity: 1;
	top: 5px;
	-webkit-transition: all 0.5s ease;
	-o-transition: all 0.5s ease;
	transition: all 0.5s ease;
::-webkit-input-placeholder
::-moz-placeholder
:-moz-placeholder
:-ms-input-placeholder
	color:#b1acac;
	opacity:1;